{
  "name": "pynodewidget-js",
  "version": "0.2.2",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "vite build --config vite.config.index.ts && vite build --config vite.config.json_schema.ts",
    "build:standalone": "vite build --config vite.config.standalone.ts",
