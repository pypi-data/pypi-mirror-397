{
  "name": "@bovine/account-api",
  "version": "0.5.12",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.gen.ts"
  },
  "author": {
    "name": "Helge"
  },
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.25.0",
    "@types/node": "^24.3.3",
    "typedoc": "^0.28.6"
  },
  "dependencies": {
    "@hey-api/openapi-ts": "^0.87.0",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "docs": "typedoc --entryPoints src/sdk.gen.ts --entryPoints src/types.gen.ts  --entryPoints src/client.gen.ts --entryPoints src/index.ts  --out docs",
    "generate": "openapi-ts   -i ../../docs/assets/schemas/openapi_account.json   -o src   -c @hey-api/client-fetch",
    "prepublishOnly": "npm run generate"
  },
  "packageManager": "yarn@4.6.0"
}
