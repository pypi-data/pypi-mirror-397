# Example .pypirc configuration file
# Copy this to ~/.pypirc and fill in your credentials
# DO NOT commit your actual .pypirc file with real credentials!

[distutils]
index-servers =
    pypi
    testpypi

[pypi]
username = __token__
password = pypi-YOUR-API-TOKEN-HERE

[testpypi]
repository = https://test.pypi.org/legacy/
username = __token__
password = pypi-YOUR-TEST-API-TOKEN-HERE
