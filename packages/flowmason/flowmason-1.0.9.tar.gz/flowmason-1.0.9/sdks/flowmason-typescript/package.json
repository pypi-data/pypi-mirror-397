{
  "name": "@flowmason/sdk",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript SDK for FlowMason pipeline orchestration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "vitest"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "flowmason",
    "sdk",
    "ai",
    "pipeline",
    "llm",
    "typescript",
    "javascript"
  ],
  "author": "FlowMason",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flowmason/flowmason.git",
    "directory": "sdks/flowmason-typescript"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
