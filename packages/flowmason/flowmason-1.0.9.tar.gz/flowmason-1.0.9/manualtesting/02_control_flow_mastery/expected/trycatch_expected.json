{
  "_description": "Expected behavior for trycatch tests",
  "success_path": {
    "try_executed": true,
    "catch_executed": false,
    "finally_executed": true
  },
  "error_path": {
    "try_executed": true,
    "catch_executed": true,
    "finally_executed": true,
    "error_context": {
      "error_message": "<error description>",
      "error_type": "<error classification>",
      "error_occurred": true
    }
  },
  "error_scope_continue": {
    "error_swallowed": true,
    "pipeline_continues": true
  },
  "error_scope_propagate": {
    "error_swallowed": false,
    "pipeline_fails": true
  }
}
