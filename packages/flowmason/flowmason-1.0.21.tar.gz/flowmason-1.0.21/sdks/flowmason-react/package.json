{
  "name": "@flowmason/react",
  "version": "0.1.0",
  "description": "React hooks for FlowMason pipeline orchestration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "vitest"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "flowmason",
    "react",
    "hooks",
    "ai",
    "pipeline",
    "llm"
  ],
  "author": "FlowMason",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flowmason/flowmason.git",
    "directory": "sdks/flowmason-react"
  }
}
