{
  "mcpServers": {
    "kytchen": {
      "command": "/opt/homebrew/bin/python3",
      "args": [
        "-m",
        "kytchen.mcp.local_server",
        "--enable-actions",
        "--timeout", "60",
        "--max-output", "20000"
      ],
      "env": {
        "PYTHONPATH": "/Volumes/VIXinSSD/kytchen"
      }
    }
  }
}
