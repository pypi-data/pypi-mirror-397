# API Configuration
DUNE_API_KEY=your_dune_api_key_here
DUNE_API_BASE_URL=https://api.dune.com/v1/
# Optional: Default user handle for 'list_user_queries'
DUNE_USER_HANDLE=your_dune_username

# Budget Limits (Deterministic Guards)
MAX_QUERIES_PER_SESSION=5
MAX_CREDITS_PER_SESSION=100
MAX_SCHEMA_CALLS_PER_SESSION=3

# Caching (seconds)
CACHE_TTL_SCHEMA=86400
CACHE_TTL_QUERY=86400
CACHE_TTL_STATUS=300

# Logging
LOG_LEVEL=INFO
