{
  "name": "StatusEvent",
  "type": "record",
  "fields": [
    { "name": "model_version", "type": "long" },
    { "name": "kafka_key", "type": "string" },
    {
      "name": "timestamp",
      "type": { "type": "long", "logicalType": "timestamp-micros" }
    },
    { "name": "author", "type": "EventAuthor" },
    {
      "name": "entity",
      "type": {
        "name": "StatusEntity",
        "type": "record",
        "fields": [
          { "name": "input", "type": "BinaryEvent" },
          { "name": "status", "type": "string" },
          { "name": "runtime", "type": "double" },
          { "name": "error", "type": "string" },
          { "name": "message", "type": "string" },
          {
            "name": "results",
            "type": {
              "type": "array",
              "items": { "name": "BinaryEvent", "type": "BinaryEvent" }
            }
          }
        ]
      }
    }
  ]
}
