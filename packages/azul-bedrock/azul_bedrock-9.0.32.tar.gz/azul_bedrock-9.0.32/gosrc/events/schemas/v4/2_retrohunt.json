{
  "name": "RetrohuntEvent",
  "type": "record",
  "fields": [
    { "name": "model_version", "type": "long" },
    { "name": "kafka_key", "type": "string" },
    {
      "name": "timestamp",
      "type": { "type": "long", "logicalType": "timestamp-micros" }
