{
  "name": "0g-compute-starter-kit",
  "version": "1.0.0",
  "description": "REST API starter kit for interacting with the 0G Compute Network",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "serve": "ts-node src/index.ts",
    "watch": "nodemon --exec ts-node src/index.ts",
    "demo": "ts-node src/demo-compute-flow.ts",
    "build:bundle": "esbuild src/index.ts --bundle --platform=node --format=cjs --target=node18 --outfile=dist/bundle.js"
  },
  "dependencies": {
    "@0glabs/0g-serving-broker": "latest",
    "@0glabs/0g-ts-sdk": "latest",
    "@types/crypto-js": "4.2.2",
    "circomlibjs": "^0.1.7",
    "crypto-js": "4.2.0",
    "ethers": "^6.13.1"
  },
  "devDependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "@types/node": "^20.0.0",
    "esbuild": "^0.25.10",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.0",
    "typescript": "^5.3.3"
  }
}
