[tool.poetry]
name = "{{ project_name }}"
version = "{{ version }}"
description = "{{ description }}"
authors = ["Generated by fast-forge"]

# This tells Poetry where to find the package's source code
packages = [{include = "{{ project_name }}", from = "src"}]

[tool.poetry.dependencies]
python = "^3.10"
fastapi = "^0.110.0"
uvicorn = "^0.23.0"
pydantic = "^2.6.2"

[tool.poetry.dev-dependencies]
pytest = "^7.3.0"
