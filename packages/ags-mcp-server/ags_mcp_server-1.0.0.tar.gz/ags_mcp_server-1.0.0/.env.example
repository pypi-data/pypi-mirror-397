# Anzo MCP Server Configuration
# Copy this file to .env and fill in your Anzo credentials

# Anzo Server Configuration
ANZO_BASE_URL=https://your-anzo-server.com
ANZO_USER=your-username
ANZO_PASSWORD=your-password

# Optional: Anzo Server Port (default: 443)
# ANZO_PORT=443

# Optional: HTTP Server Configuration (for anzo-mcp-http)
# MCP_HTTP_HOST=127.0.0.1
# MCP_HTTP_PORT=8000

# PyAnzo Configuration (for SPARQL queries)
PYANZO_DOMAIN=your-anzo-server.com
PYANZO_PORT=443
PYANZO_USER=your-username
PYANZO_PASSWORD=your-password
PYANZO_USE_SSL=true
