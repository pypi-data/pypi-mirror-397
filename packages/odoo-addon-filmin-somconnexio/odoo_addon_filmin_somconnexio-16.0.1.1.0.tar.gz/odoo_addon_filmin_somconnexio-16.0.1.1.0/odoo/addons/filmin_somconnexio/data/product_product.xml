<?xml version="1.0"?>
<odoo>
  <data noupdate="1">
    <record id="FilminSubscription" model="product.product">
      <field name="name">Subscription Filmin</field>
      <field name="product_tmpl_id" ref="multimedia_somconnexio.streaming_product_template"/>
      <field name="product_template_attribute_value_ids" search="[('product_attribute_value_id', 'in', [ref('multimedia_somconnexio.12MonthDuration'), ref('somconnexio.ParticularExclusive'), ref('somconnexio.AddOn')]), ('product_tmpl_id', '=', ref('multimedia_somconnexio.streaming_product_template'))]"/>
      <field name="default_code">SE_SC_REC_BA_CT_FLM</field>
      <field name="custom_name">Subscription Filmin</field>
      <field name="subscription_template_id" ref="filmin_somconnexio.sale_subscription_template_filmin"/>
      <!-- Storable product -->
      <field name="detailed_type">product</field>
      <!-- Tracking code -->
      <field name="tracking">serial</field>
    </record>
  </data>
</odoo>
