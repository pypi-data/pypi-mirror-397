{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "prettier --write .",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run src/tests --exclude src/tests/hooks/useStreamingChat.integration.test.ts",
    "test:integration": "RUN_LIVE_CHAT=1 vitest run src/tests/hooks/useStreamingChat.integration.test.ts"
  },
  "dependencies": {
    "@openai/apps-sdk-ui": "^0.2.0",
