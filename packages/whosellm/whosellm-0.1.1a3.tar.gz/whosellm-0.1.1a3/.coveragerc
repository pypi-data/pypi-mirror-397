# Coverage 配置文件 / Coverage configuration file

[run]
source = llmeta
omit = 
    */tests/*
    */__pycache__/*
    */site-packages/*
    .venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
