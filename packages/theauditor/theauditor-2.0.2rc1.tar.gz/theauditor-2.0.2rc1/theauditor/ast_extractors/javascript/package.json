{
  "name": "@theauditor/js-extractors",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "esbuild src/main.ts --bundle --platform=node --target=node18 --format=cjs --outfile=dist/extractor.cjs",
    "test": "tsx src/test_extractor.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "pino": "10.1.0",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@types/node": "25.0.0",
    "esbuild": "0.27.1",
    "ts-node": "10.9.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  }
}
