<Schema name="oec" default_locale="en">
    <Cube name="customers" caption="Customers">
        <Annotation name="dataset_name">Customers</Annotation>

        <Table name="customers" primary_key="customer_id" />

        <Dimension name="Customer ID" foreign_key="customer_id">
            <Hierarchy name="Customer ID">
                <Level name="Customer ID" key_column="customer_id"/>
            </Hierarchy>
        </Dimension>

        <Dimension name="Customer Name" foreign_key="customer_name">
            <Hierarchy name="Customer Name">
                <Level name="Customer Name" key_column="customer_name" />
            </Hierarchy>
        </Dimension>

        <Dimension name="City" foreign_key="city">
            <Hierarchy name="City">
                <Level name="City" key_column="city" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Country" foreign_key="country">
            <Hierarchy name="Country">
                <Level name="Country" key_column="country" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Membership Tier" foreign_key="membership_tier">
            <Hierarchy name="Membership Tier">
                <Level name="Membership Tier" key_column="membership_tier" />
            </Hierarchy>
        </Dimension>

        <Measure name="Count" key_column="count" aggregator="sum" />
    </Cube>
</Schema>