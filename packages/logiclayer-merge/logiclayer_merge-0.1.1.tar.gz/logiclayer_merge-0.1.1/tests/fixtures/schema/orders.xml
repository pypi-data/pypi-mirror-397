<Schema name="oec" default_locale="en">
    <Cube name="orders" caption="Orders Table">
        <Annotation name="dataset_name">Orders Table</Annotation>

        <Table name="orders" primary_key="order_id" />

        <Dimension name="Order ID" foreign_key="order_id">
            <Hierarchy name="Order ID">
                <Level name="Order ID" key_column="order_id" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Customer ID" foreign_key="customer_id">
            <Hierarchy name="Customer ID">
                <Level name="Customer ID" key_column="customer_id" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Product" foreign_key="product">
            <Hierarchy name="Product">
                <Level name="Product" key_column="product" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Order Date" foreign_key="order_date">
            <Hierarchy name="Order Date">
                <Level name="Order Date" key_column="order_date" />
            </Hierarchy>
        </Dimension>

        <Measure name="Quantity" key_column="quantity" aggregator="sum" />
        <Measure name="Unit Price" key_column="unit_price" aggregator="sum" />
    </Cube>
</Schema>