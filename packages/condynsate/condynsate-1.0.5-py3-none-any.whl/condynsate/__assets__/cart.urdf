<?xml version="1.0" ?>
<robot name="cart">


    <!-- ########### MATERIALS ########### -->

    <material name="industrial">
        <color rgba="0.114 0.345 0.655 1" />
    </material>
    
    <material name="arches">
        <color rgba="0 0.624 0.831 1" />
    </material>
    
    <material name = "heritage">
        <color rgba = "0.961 0.510 0.118 1" />
    </material>
  

    <!-- ########### LINKS ########### -->

    <link name="chassis">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Cube_1x1x1_lower_origin.stl" scale="1 0.5 0.25"/>
            </geometry>
            <material name = "industrial"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Cube_1x1x1_lower_origin.stl" scale="1 0.5 0.25"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="2"/>
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>

    <link name="arm">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Cylinder_1x1_lower_origin.stl" scale="0.05 0.05 1"/>
            </geometry>
            <material name = "arches"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Cylinder_1x1_lower_origin.stl" scale="0.05 0.05 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 1"/>
            <mass value="0.0"/>
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>

    <link name="mass">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Sphere_1_center_origin.stl" scale="0.2 0.2 0.2"/>
            </geometry>
            <material name = "arches"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Sphere_1_center_origin.stl" scale="0.2 0.2 0.2"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="1"/>
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>

    <link name="wheel_1">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Cylinder_1x1_lower_origin.stl" scale="0.25 0.25 0.05"/>
            </geometry>
            <material name = "heritage"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Cylinder_1x1_lower_origin.stl" scale="0.25 0.25 0.05"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.25"/>
            <inertia ixx="0.00390625" ixy="0" ixz="0" iyy="0.00390625" iyz="0" izz="0.002005208"/>
        </inertial>
    </link>

    <link name="wheel_2">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Cylinder_1x1_lower_origin.stl" scale="0.25 0.25 0.05"/>
            </geometry>
            <material name = "heritage"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Cylinder_1x1_lower_origin.stl" scale="0.25 0.25 0.05"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.25"/>
            <inertia ixx="0.00390625" ixy="0" ixz="0" iyy="0.00390625" iyz="0" izz="0.002005208"/>
        </inertial>
    </link>

    <link name="wheel_3">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Cylinder_1x1_lower_origin.stl" scale="0.25 0.25 0.05"/>
            </geometry>
            <material name = "heritage"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Cylinder_1x1_lower_origin.stl" scale="0.25 0.25 0.05"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.25"/>
            <inertia ixx="0.00390625" ixy="0" ixz="0" iyy="0.00390625" iyz="0" izz="0.002005208"/>
        </inertial>
    </link>

    <link name="wheel_4">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Cylinder_1x1_lower_origin.stl" scale="0.25 0.25 0.05"/>
            </geometry>
            <material name = "heritage"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="Cylinder_1x1_lower_origin.stl" scale="0.25 0.25 0.05"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.25"/>
            <inertia ixx="0.00390625" ixy="0" ixz="0" iyy="0.00390625" iyz="0" izz="0.002005208"/>
        </inertial>
    </link>



    <!-- ########### JOINTS ########### -->

    <joint name="chassis_to_arm" type="continuous">
        <parent link = "chassis" />
        <child link = "arm" />
        <origin rpy="0 0 0" xyz = "0 0 0.25" />
        <axis xyz = "0 -1 0" />
    </joint>

    <joint name="arm_to_mass" type="fixed">
        <parent link = "arm" />
        <child link = "mass" />
        <origin rpy="0 0 0" xyz = "0 0 1" />
    </joint>

    <joint name="chassis_to_wheel_1" type="continuous">
        <parent link = "chassis" />
        <child link = "wheel_1" />
        <origin rpy="1.5708 0 0" xyz = "-0.375 -0.251 0" />
        <axis xyz = "0 0 -1" />
    </joint>

    <joint name="chassis_to_wheel_2" type="continuous">
        <parent link = "chassis" />
        <child link = "wheel_2" />
        <origin rpy="1.5708 0 0" xyz = "0.375 -0.251 0" />
        <axis xyz = "0 0 -1" />
    </joint>

    <joint name="chassis_to_wheel_3" type="continuous">
        <parent link = "chassis" />
        <child link = "wheel_3" />
        <origin rpy="-1.5708 0 0" xyz = "-0.375 0.251 0" />
        <axis xyz = "0 0 1" />
    </joint>

    <joint name="chassis_to_wheel_4" type="continuous">
        <parent link = "chassis" />
        <child link = "wheel_4" />
        <origin rpy="-1.5708 0 0" xyz = "0.375 0.251 0" />
        <axis xyz = "0 0 1" />
    </joint>


</robot>

