<?xml version="1.0" ?>
<robot name="plane_small">


    <!-- ########### MATERIALS ########### -->

    <material name="steel">
        <color rgba="0.75 0.75 0.75 1"/>
    </material>


    <!-- ########### LINKS ########### -->

      <link name="plane">
          <visual>
              <origin rpy="0 0 0" xyz="0 0 0"/>
                  <geometry>
                      <mesh filename="plane.obj" scale="1 1 1"/>
                  </geometry>
              <material name = "steel"/>
          </visual>
      <collision>
          <origin rpy="0 0 0" xyz="0 0 -0.5"/>
          <geometry>
              <box size="1 1 1"/>
          </geometry>
      </collision>
      <inertial>
          <origin rpy="0 0 0" xyz="0 0 0"/>
          <mass value="0"/>
          <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
      </inertial>
  </link>


</robot>

