{
	"template": "<YEAR>.<MONTH>.<COUNTER>",
	"version": "2025.12.13",
	"YEAR": 2025,
	"MONTH": 12,
	"COUNTER": 13,
	"targets": {
		"pyproject.toml": {
			"find": "\"<VERSION>\"\\s*#\\s*version",
			"replace": "\"<VERSION>\" # version"
		},
		"messthaler_wulff/version.py": {
			"find": ".+",
			"replace": "program_version = \"<VERSION>\"\n"
		}
	}
}