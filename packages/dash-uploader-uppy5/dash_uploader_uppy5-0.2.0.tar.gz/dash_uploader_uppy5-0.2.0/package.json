{
  "name": "dash_uploader_uppy5",
  "version": "0.2.0",
  "description": "A modern Dash file uploader component powered by Uppy 5",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/Ozx-68102/dash-uploader-uppy5.git"
  },
  "bugs": {
    "url": "https://github.com/Ozx-68102/dash-uploader-uppy5/issues"
  },
  "homepage": "https://github.com/Ozx-68102/dash-uploader-uppy5",
  "scripts": {
    "build:js::dev": "webpack --mode development",
    "build:js": "webpack",
    "build:py": "dash-generate-components ./src/components dash_uploader_uppy5 -p package-info.json  --ignore \\.test\\.",
    "build:postpy": "python devscripts/post_build.py",
    "build": "pnpm run build:js && pnpm run build:py && pnpm run build:postpy",
    "watch": "pnpm run build:js::dev -- --watch",
    "dist": "python -m build --sdist --wheel --outdir dist/",
    "format": "prettier --write src --ignore-path=.prettierignore"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "css-loader": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "react-docgen": "^8.0.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@uppy/core": "^5.2.0",
    "@uppy/dashboard": "^5.1.0",
    "@uppy/react": "^5.1.1",
    "@uppy/xhr-upload": "^5.1.0"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "author": "Zhixin Ou <ozx1072960726@gmail.com>",
  "license": "MIT"
}
