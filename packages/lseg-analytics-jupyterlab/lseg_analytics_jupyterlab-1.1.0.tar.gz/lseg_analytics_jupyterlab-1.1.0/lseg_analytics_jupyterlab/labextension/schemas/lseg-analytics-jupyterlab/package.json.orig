{
  "name": "lseg-analytics-jupyterlab",
  "version": "1.1.0",
  "description": "Provides coding assistance for the LSEG Analytics Pricing Python SDK.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "lseg",
    "analytics"
  ],
  "author": "LSEG.Publishers",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "client/src/**/*.{ts,tsx}",
    "lseg_analytics_jupyterlab/**/*.{py, json}",
    "schema/*.json"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "style": "style/index.css",
  "scripts": {
    "start": "npm run build:dist && jupyter lab --config jupyter_server_test_config.py",
    "start:server": "jupyter server --config=jupyter_config/server_config/lseg_analytics_jupyterlab.py",
    "build:server": "python -m build",
    "build": "cross-env NODE_ENV=development npx tsc -b && npm run build:dist && npm run build:labextension:dev",
    "build:prod": "cross-env NODE_ENV=production npx tsc -b && npm run clean && npm run build:dist:prod && npm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:dist": "tsc --sourceMap",
    "build:dist:prod": "tsc",
    "clean": "npm run clean:dist",
    "clean:dist": "rimraf dist tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "clean:labextension": "rimraf lseg_analytics_jupyterlab/labextension lseg_analytics_jupyterlab/_version.py",
    "clean:all": "npm run clean:dist && npm run clean:labextension && npm run clean:lintcache",
    "eslint": "cross-env NODE_OPTIONS=\"--max-old-space-size=16384\" npm run eslint:check -- --fix",
    "eslint:check": "cross-env NODE_OPTIONS=\"--max-old-space-size=16384\" eslint client/src --ext .ts,.tsx",
    "install:extension": "npm run build",
    "preformat:check": "prettier ../shared --check",
    "preformat": "prettier ../shared --write",
    "lint": "npm run stylelint && npm run preformat && npm run format && npm run eslint",
    "lint:check": "npm run stylelint:check && npm run preformat:check && npm run format:check && npm run eslint:check",
    "prettier": "prettier client/src --write",
    "prettier:check": "prettier client/src --check",
    "black": "black lseg_analytics_jupyterlab/**/*.py",
    "black:check": "black --check lseg_analytics_jupyterlab/**/*.py",
    "format": "npm run prettier && npm run black",
    "format:check": "npm run prettier:check && npm run black:check",
    "stylelint": "npm run stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w --sourceMap",
    "watch:labextension": "jupyter labextension watch .",
    "test": "npm run lint:check && jest test/unit --runInBand && npm run test:server",
    "test:server": "pytest --cov=lseg_analytics_jupyterlab",
    "test:only": "jest test/unit --runInBand",
    "test:coverage": "jest --coverage test/unit && npm run test:server",
    "test:coverage:ci": "jest test/unit --coverage --ci --reporters=default --reporters=jest-junit --runInBand --config ./jest.config.js && pytest --cov=lseg_analytics_jupyterlab --cov-report=xml:coverage/coverage-python.xml --junitxml=coverage/junit/python_junit.xml"
  },
  "dependencies": {
    "@fluentui/react": "^8.122.9",
    "@fluentui/react-components": "^9.60.0",
    "@jupyterlab/application": "^4.4.0",
    "@jupyterlab/cells": "^4.4.0",
    "@jupyterlab/completer": "^4.4.0",
    "@jupyterlab/console": "^4.4.0",
    "@jupyterlab/docmanager": "^4.4.0",
    "@jupyterlab/fileeditor": "^4.4.0",
    "@jupyterlab/logconsole": "^4.4.0",
    "@jupyterlab/lsp": "^4.4.0",
    "@jupyterlab/notebook": "^4.4.0",
    "@jupyterlab/services": "^7.4.0",
    "@jupyterlab/statedb": "^4.4.0",
    "@jupyterlab/testutils": "^4.4.0",
    "@jupyterlab/translation": "^4.4.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "inversify": "6.0.2",
    "marked": "^17.0.1",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "reflect-metadata": "^0.2.2",
    "shared": "*",
    "ts-loader": "^9.5.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.4.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/events": "^3.0.3",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^20.14.2",
    "@types/react": "~18.0.26",
    "@types/react-addons-linked-state-mixin": "^0.14.22",
    "@types/react-dom": "~18.0.11",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.1.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsdoc": "^50.6.2",
    "eslint-plugin-prettier": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.5",
    "source-map-loader": "^1.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-csstree-validator": "^3.0.0",
    "stylelint-prettier": "^4.0.0",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "5.2.2",
    "webpack-stats-plugin": "^1.1.3",
    "yjs": "^13.5.0"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "lseg_analytics_jupyterlab/labextension",
    "schemaDir": "schema",
    "webpackConfig": "./webpack.config.js"
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "coverage",
    "**/*.d.ts"
  ],
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-standard",
      "stylelint-prettier/recommended"
    ],
    "plugins": [
      "stylelint-csstree-validator"
    ],
    "rules": {
      "csstree/validator": true,
      "property-no-vendor-prefix": null,
      "selector-class-pattern": "^([a-z][A-z\\d]*)(-[A-z\\d]+)*$",
      "selector-no-vendor-prefix": null,
      "value-no-vendor-prefix": null
    }
  }
}