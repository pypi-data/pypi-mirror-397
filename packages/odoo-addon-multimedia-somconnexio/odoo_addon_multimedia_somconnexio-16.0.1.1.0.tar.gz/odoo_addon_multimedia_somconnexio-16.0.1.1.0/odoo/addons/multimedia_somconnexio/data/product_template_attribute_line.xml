<?xml version="1.0"?>
<odoo>
  <data noupdate="1">
    <!-- TODO: (review) The only purpose of this attribute is to avoid the violation of unique constraint "product_product_combination_unique" during product creation
         DETAIL: Key (product_tmpl_id, combination_indices)=(114, ) already exists.
    -->
    <record id="Streaming_product_template_DurationMonths_attribute_line" model="product.template.attribute.line">
      <field name="product_tmpl_id" ref="streaming_product_template"/>
      <field name="attribute_id" ref="DurationMonths"/>
      <field name="value_ids" eval="[(6, 0, [ref('12MonthDuration')])]"/>
    </record>
    <record id="Streaming_product_template_SalesCategory_attribute_line" model="product.template.attribute.line">
      <field name="product_tmpl_id" ref="streaming_product_template"/>
      <field name="attribute_id" ref="somconnexio.SalesCategory"/>
      <field name="value_ids" eval="[(6, 0, [ref('somconnexio.ParticularExclusive'), ref('somconnexio.CompanyExclusive')])]"/>
    </record>
    <record id="Streaming_product_template_IsAddOn_attribute_line" model="product.template.attribute.line">
      <field name="product_tmpl_id" ref="streaming_product_template"/>
      <field name="attribute_id" ref="somconnexio.IsAddOn"/>
      <field name="value_ids" eval="[(6, 0, [ref('somconnexio.AddOn')])]"/>
    </record>
  </data>
</odoo>
